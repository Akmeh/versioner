{
  "name": "akmeh/versioning",
  "description": "Version Manager in order to generate .version in every version and keep tracking on it",
  "license": "proprietary",
  "authors": [
    {
      "name": "Akmeh"
    }
  ],
  "require": {
    "symfony/console": "^3.3",
    "league/flysystem" : "^1.0",
    "symfony/process" : "^3.3"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "mockery/mockery" : "0.9.5"
  },
  "bin": [
        "src/version"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "bin": ["version"],

  "autoload": {
    "psr-4": {
      "Versioning\\": "src/"
    }
  }
}



