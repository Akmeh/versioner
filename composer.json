{
  "name": "akmeh/versioning",
  "description": "Version Manager in order to generate .version in every version and keep tracking on it",
  "license": "proprietary",
  "authors": [
    {
      "name": "Akmeh"
    }
  ],
  "require": {
    "symfony/console": "3.1.*",
    "league/flysystem" : "^1.0"
  },
  "require-dev": {
    "codeception/codeception": "dev-master",
    "mockery/mockery" : "0.9.5"
  },
  "bin": [
        "src/version"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Versioning\\": "src/"
    }
  }
}



